# Automatically generated do not edit

no-resolv
no-hosts
keep-in-foreground
{{ if .log_queries }}
log-queries
{{ end }}
{{ if .log_dhcp }}
log-dhcp
{{ end }}
log-facility=-
no-poll
user=root
cache-size={{ .cache_size}}

# Default forward servers
{{ range .defaults }}
server={{ . }}
{{ end }}

# Domain forwards
{{ range .forwards }}
server=/{{ .domain }}/{{ .server }}
{{ end }}

# Static hosts
{{ range .hosts }}
address=/{{ .host }}/{{ .ip }}
{{ end }}

# Static srv-hosts
{{ range .services }}
srv-host={{ .srv }},{{ .host }},{{ .port }},{{ .priority }},{{ .weight }}
{{ end }}

# Static cname
{{ range .cnames }}
cname={{ .name }},{{ .target }}
{{ end }}

{{ if .dhcp }}
dhcp-range={{ .dhcp_range }},12h
dhcp-option=option:netmask,{{ .dhcp_netmask }}
dhcp-option=option:router,{{ .dhcp_router }}
dhcp-option=option:dns-server,{{ .dhcp_dns }}
dhcp-option=option:ntp-server,{{ .dhcp_ntp }}
dhcp-option=option:domain-name,{{ .dhcp_domain }}
{{ end }}